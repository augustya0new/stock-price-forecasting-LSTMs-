    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "df = pd.read_csv(\"../data/stock_prices.csv\")\n",
    "\n",
    "plt.figure(figsize=(12,6))\n",
    "plt.plot(df[\"Date\"], df[\"Close\"], label=\"Closing Price\")\n",
    "plt.xlabel(\"Date\")\n",
    "plt.ylabel(\"Price\")\n",
    "plt.title(\"Stock Closing Price Trend\")\n",
    "plt.legend()\n",
    "plt.show()"
   ]
  }
